<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.locacenter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Locacenter">
        <activity android:name=".model.BancoController"></activity>
        <activity android:name=".controller.exibir.AddCasaOuInquelino" />
        <activity android:name=".controller.exibir.AdicionarCasaForm" />
        <activity android:name=".controller.exibir.AdicionarInquelinoForm" />
        <activity android:name=".controller.exibir.Exibir" />
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>